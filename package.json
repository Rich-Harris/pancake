{
	"name": "@sveltejs/pancake",
	"version": "0.0.18",
	"description": "Experimental charting library for Svelte",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"release": "npm run package && cd package && npm publish",
		"deploy": "npm run build && surge build pancake-charts.surge.sh",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/Rich-Harris/pancake.git"
	},
	"keywords": [
		"svelte",
		"svg",
		"charts",
		"dataviz"
	],
	"author": "Rich Harris",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Rich-Harris/pancake/issues"
	},
	"homepage": "https://github.com/Rich-Harris/pancake#readme",
	"dependencies": {
		"yootils": "0.3.1"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.16",
		"@sveltejs/kit": "next",
		"d3-hierarchy": "^3.0.1",
		"httpie": "^1.1.2",
		"prettier": "~2.3.2",
		"prettier-plugin-svelte": "^2.3.1",
		"svelte": "^3.42.1",
		"svelte2tsx": "^0.4.5",
		"typescript": "^4.3.5"
	},
	"type": "module"
}
